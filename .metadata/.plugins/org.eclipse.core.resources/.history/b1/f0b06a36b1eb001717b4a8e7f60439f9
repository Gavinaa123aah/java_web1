		int[] a = {2,4,6,7,8,9,11,23,45,67};
		System.out.println(binarySearch(a, 11));

	}
	 public static int binarySearch(int arr[], int data){
	     int low = 0;
	     int high = arr.length-1;

	     while(low < high){
	     int mid = (low + high)/2;
	     if(data == arr[mid]){
	      return mid;
	     }else if(data < arr[mid]){
	      high = mid - 1;
	     }else{
	      low = mid + 1;
	     }
	     }

	     return -1;

	}
}
