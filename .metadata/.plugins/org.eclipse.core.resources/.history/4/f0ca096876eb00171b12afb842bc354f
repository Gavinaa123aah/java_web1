package com.exercise.example;

public class D {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int[] a = null;
		a = new int[10];
		for (int i = 0; i < a.length; i++) {
			a[i] = i+1;
		}
		
		digui(a, 0);
	}
	
	public static void digui(int arr[],int index) {
		
		if(index!=arr.length){
		digui(arr, index++);
		
		System.out.print(" "+arr[index]);
		}
	}
}
